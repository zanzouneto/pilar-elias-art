{
  "$schema": "https://beta.frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "painting",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Content preview",
          "name": "image",
          "type": "image"
        },
        {
          "title": "Is it available for sale?",
          "name": "forSale",
          "type": "boolean",
          "default": "true"
        },
        {
          "title": "PriceInDollars",
          "name": "price",
          "type": "number"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "other",
  "frontMatter.content.publicFolder": "",
  "frontMatter.content.pageFolders": [
    {
      "title": "cms",
      "path": "[[workspace]]/cms"
    }
  ]
}